{
  "name": "codaisseur-ts-starter",
  "version": "1.0.0",
  "description": "Codaisseur's TypeScript Starter Template",
  "repository": "https://github.com/Codaisseur/ts-starter.git",
  "author": "Codaisseur <support@codaisseur.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/",
    "dev": "npm-run-all -p server:dev client:dev",
    "server:dev": "DEBUG=${DEBUG:-'app:*'} nodemon --inspect dist/",
    "client:dev": "node client/scripts/start.js",
    "client:build": "node client/scripts/build.js",
    "client:test": "node client/scripts/test.js --env=jsdom",
    "db:create": "node dist/bin/db.js create",
    "db:drop": "node dist/bin/db.js drop",
    "db:sync": "node dist/bin/db.js sync",
    "db:sync:force": "node dist/bin/db.js sync --force",
    "db:migration:generate": "node_modules/.bin/sequelize migration:generate --underscored --name"
  },
  "engines": {
    "node": ">=8"
  },
  "jest": {
    "collectCoverageFrom": [
      "dist/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/client/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/dist/**/__tests__/**/*.js?(x)",
      "<rootDir>/dist/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/client/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/client/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "dependencies": {
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.0.3",
    "babel-runtime": "6.26.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.18.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "cors": "^2.8.4",
    "css-loader": "0.28.4",
    "debug": "^3.0.1",
    "dotenv": "4.0.0",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "helmet": "^3.8.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "morgan": "^1.8.2",
    "object-assign": "4.1.1",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "react": "^16.0.0",
    "react-dev-utils": "^4.1.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reflect-metadata": "^0.1.10",
    "sequelize": "^4.10.3",
    "sequelize-typescript": "^0.5.0",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-node": "^3.3.0",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.8.2",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.11",
    "@types/body-parser": "^1.16.5",
    "@types/cors": "^2.8.1",
    "@types/debug": "^0.0.30",
    "@types/express": "^4.0.37",
    "@types/helmet": "^0.0.36",
    "@types/morgan": "^1.7.33",
    "@types/pg": "^7.1.2",
    "@types/react": "^16.0.7",
    "@types/react-dom": "^15.5.5",
    "@types/react-redux": "^5.0.9",
    "@types/reflect-metadata": "^0.0.5",
    "@types/sequelize": "^4.0.74",
    "@types/winston": "^2.3.6",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "sequelize-cli": "^3.0.0",
    "tslint": "^5.7.0",
    "typescript": "^2.6.0-dev.20170923"
  }
}
